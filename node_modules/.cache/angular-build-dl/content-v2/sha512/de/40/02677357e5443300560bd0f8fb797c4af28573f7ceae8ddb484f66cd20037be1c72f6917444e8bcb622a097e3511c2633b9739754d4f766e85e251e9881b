        styles: [tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"](__webpack_require__(/*! ./styles/getting-started.page.scss */ "./src/app/getting-started/styles/getting-started.page.scss")).default, tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"](__webpack_require__(/*! ./styles/getting-started.shell.scss */ "./src/app/getting-started/styles/getting-started.shell.scss")).default, tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"](__webpack_require__(/*! ./styles/getting-started.responsive.scss */ "./src/app/getting-started/styles/getting-started.responsive.scss")).default]
