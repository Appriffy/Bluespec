    getSelectedJobTypesCount() {
        const selectedJobTypes = [];
        this.postJobForm.value.job_types
            .map((value, index) => {
            if (value) {
                selectedJobTypes.push(this.jobTypes[index].id);
            }
        });
        return selectedJobTypes.length;
    }
