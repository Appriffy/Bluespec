                this.postJobForm.get('max_rate_per_hour').clearValidators();
                this.postJobForm.get('max_rate_per_hour').updateValueAndValidity();
                break;
            case "perHour":
                this.postJobForm.get('min_rate_per_hour').setValidators(_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required);
                this.postJobForm.get('max_rate_per_hour').setValidators(_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required);
