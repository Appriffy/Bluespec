                    console.log("Start the Restore ##1: " + JSON.stringify(this.filterArr.selected_skills_arr));
                    this.skill_types.map((item) => {
                        if (this.filterArr.selected_skills_arr.includes(item.id)) {
                            item.enabled = 1;
                        }
                        else {
                            item.enabled = 0;
                        }
                    });
                }
                //End of restoration
            }
        });
        /********* DELETE ALSO THIS ******/
