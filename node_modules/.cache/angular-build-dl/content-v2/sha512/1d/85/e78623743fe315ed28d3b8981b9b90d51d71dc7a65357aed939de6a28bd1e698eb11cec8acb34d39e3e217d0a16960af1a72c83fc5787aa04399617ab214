let JobApplicationsManagerResolver = class JobApplicationsManagerResolver {
    constructor(userService, storageService, authService) {
        this.userService = userService;
        this.storageService = storageService;
        this.authService = authService;
    }
    resolve(route) {
        return tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"](this, void 0, void 0, function* () {
            let authDataVal = yield this.storageService.get(_config_AuthConstants__WEBPACK_IMPORTED_MODULE_4__["AuthConstants"].AUTH);
            //console.log("JobApplicationsManagerResolver authDataVal => "+JSON.stringify(authDataVal));
            let jobCode = route.params['job_code'];
            let apiKey = authDataVal.api_key;
            //console.log("JobApplicationsManagerResolver apiKey => "+apiKey);
            let postData = { job_id: jobCode };
            const dataSource = this.userService.getJobApplicationsManagerDataSource(apiKey, postData);
            const dataStore = this.userService.getJobApplicationsManagerStore(dataSource);
            return dataStore;
        });
    }
};
